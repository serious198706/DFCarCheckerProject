                    android:layout_width="0dp"
                    android:layout_height="35dp"
                    android:text="@string/choose"
                    android:textColor="#FFF"
                    android:background="@drawable/button_custom"
                    android:layout_weight="1"/>
            </TableRow>

            <TableRow>
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:text="@string/ct_business_insurance"
                    android:layout_weight="1"
                    android:textSize="18sp"/>
                <Spinner android:id="@+id/ct_insurance_spinner"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:entries="@array/ct_insurance_items"/>
            </TableRow>
            <TableLayout android:id="@+id/ct_insurance_table"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TableRow>
                    <TextView
                        android:layout_height="wrap_content"
                        android:layout_width="0dp"
                        android:text="@string/ct_business_insurance_area"
                        android:layout_weight="1"
                        android:textSize="18sp"/>
                    <Spinner android:id="@+id/ct_insuranceRegion_spinner"
                        android:layout_height="wrap_content"
                        android:layout_width="0dp"
                        android:layout_weight="2"
                        android:entries="@array/ct_insuranceRegion_items"/>
                </TableRow>
                <TableRow>
                    <TextView
                        android:layout_height="wrap_content"
                        android:layout_width="0dp"
                        android:text="@string/ct_business_insurance_money"
                        android:layout_weight="1"
                        android:textSize="18sp"/>
                    <EditText android:id="@+id/ct_insuranceAmount_edit"
                        android:layout_height="wrap_content"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:maxLength="7"
                        android:inputType="number"/>
                    <TextView android:layout_height="wrap_content"
                        android:layout_width="0dp"
                        android:text="@string/ci_money"
                        android:layout_weight="1"/>
                </TableRow>
                <TableRow>
                    <TextView
                        android:layout_height="wrap_content"
                        android:layout_width="0dp"
                        android:text="@string/ct_business_insurance_available_date"
                        android:layout_weight="1"
                        android:textSize="18sp"/>
                    <Spinner android:id="@+id/ct_insuranceExpiryYear_spinner"
                        android:layout_height="wrap_content"
                        android:layout_width="0dp"
                        android:layout_weight="1"/>
                    <Spinner android:id="@+id/ct_insuranceExpiryMonth_spinner"
                        android:layout_height="wrap_content"
                        android:layout_width="0dp"
                        android:layout_weight="1"/>
                </TableRow>
                <TableRow>
                    <TextView
                        android:layout_height="wrap_content"
                        android:layout_width="0dp"
                        android:text="@string/ct_business_insurance_company"
                        android:layout_weight="1"
                        android:textSize="18sp"/>
                    <Spinner android:id="@+id/ct_insuranceCompany_spinner"
                        android:layout_height="wrap_content"
                        android:layout_width="0dp"
                        android:layout_weight="2"
                        android:entries="@array/ct_insuranceCompany_items"/>
                </TableRow>
            </TableLayout>

            <TableRow android:id="@+id/ct_ported_procedure"
                android:visibility="gone">
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:text="@string/ct_ported_procedure"
                    android:layout_weight="1"
                    android:textSize="18sp"/>
                <Spinner android:id="@+id/ct_importProcedures_spinner"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:entries="@array/ct_importProcedures_items"/>
            </TableRow>
            <TableRow>
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:text="@string/ct_backup_tire"
                    android:layout_weight="1"
                    android:textSize="18sp"/>
                <Spinner android:id="@+id/ct_spareTire_spinner"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:entries="@array/ct_spareTire_items"/>
            </TableRow>

            <TableRow>
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:text="@string/ct_backup_key"
                    android:layout_weight="1"
                    android:textSize="18sp"/>
                <Spinner android:id="@+id/ct_spareKey_spinner"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_weight="2"
                    android:entries="@array/existornot"/>
            </TableRow>
            <TableRow>
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:text="@string/ci_seller_name"
                    android:layout_weight="1"
                    android:textSize="18sp"/>
                <EditText android:id="@+id/ci_ownerName_edit"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_weight="2"/>
            </TableRow>
            <TableRow>
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:text="@string/ci_seller_id_number"
                    android:layout_weight="1"
                    android:textSize="18sp"/>
                <EditText android:id="@+id/ci_ownerIdNumber_edit"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_weight="2"/>
            </TableRow>
            <TableRow>
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:text="@string/ci_seller_phone_number"
                    android:layout_weight="1"
                    android:textSize="18sp"/>
                <EditText android:id="@+id/ci_ownerPhone_edit"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:inputType="phone"
                    android:layout_weight="2"
                    android:maxLength="13"/>
            </TableRow>
            <TableRow  android:visibility="gone">
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:text="@string/ex_exchange_type"
                    android:layout_weight="2"
                    android:textSize="18sp"/>
                <Spinner android:id="@+id/ex_transferAgree_spinner"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_weight="4"
                    android:entries="@array/ex_transferAgree_items"/>
            </TableRow>
            <!-- 手续录入部分结束 -->

            <TextView  android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:drawableLeft="@drawable/comment_item"
                android:drawablePadding="10dp"
                style="@style/CodeFont"
                android:text="@string/comment" />

            <View  android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#000"/>
            <TableRow android:visibility="gone">
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:text="@string/cbi_comment"
                    android:layout_weight="1"
                    android:textSize="18sp"/>
                <EditText android:id="@+id/ct_transferRequire_edit"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:inputType="textMultiLine"
                    android:ems="10"/>
            </TableRow>

            <ImageView android:visibility="gone"
                android:id="@+id/imageFileName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:contentDescription="@string/itemImage"/>
        </TableLayout>
    </LinearLayout>
</ScrollView>

