<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:id="@+id/root">


    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp">
        <TextView
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="@string/bi_vin"
            android:drawableLeft="@drawable/basic_info_item"
            android:drawablePadding="10dp"
            style="@style/CodeFont"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#000"/>

        <LinearLayout android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/vin"
                android:textSize="22sp"
                style="@style/CodeFont"/>
            <Spinner android:id="@+id/bi_vin_spinner"
                android:layout_height="wrap_content"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:visibility="gone"/>
            <EditText android:id="@+id/bi_vin_edit"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:inputType="textCapCharacters"/>
            <!-- LE4FG65Z487015744 -->
            <Button android:id="@+id/bi_vin_button"
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:layout_weight="1"
                android:text="@string/ok"
                android:textColor="#FFF"
                android:background="@drawable/button_custom"
                android:padding="-5dp" />
        </LinearLayout>
        <LinearLayout android:id="@+id/brand_input"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="gone">
            <TextView android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/brand"
                android:textSize="22sp"
                style="@style/CodeFont"/>
            <EditText android:id="@+id/bi_brand_edit"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:enabled="false"/>
            <Button android:id="@+id/bi_brand_ok_button"
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:layout_weight="1"
                android:layout_marginRight="1dp"
                android:text="@string/ok"
                android:textColor="#FFF"
                android:background="@drawable/button_custom"
                android:padding="-5dp" />
            <Button android:id="@+id/bi_brand_select_button"
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:layout_weight="1"
                android:layout_marginLeft="1dp"
                android:text="@string/choose"
                android:background="@drawable/button_custom1"
                android:padding="-5dp" />
        </LinearLayout>


        <TableLayout android:id="@+id/bi_content_table"
